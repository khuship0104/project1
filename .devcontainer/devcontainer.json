{
  "name": "Julia",
  "image": "juliadocker/julia:1.10-bullseye",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {}
  },
  "postCreateCommand": "julia -e 'using Pkg; Pkg.activate(\".\"); Pkg.instantiate()'",
  "customizations": {
    "vscode": {
      "extensions": ["julialang.language-julia"]
    }
  }
}
